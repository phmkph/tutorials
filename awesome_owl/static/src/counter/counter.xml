<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="awesome_owl.Counter">
		<div class="m-2 p-2 border d-inline-block">
			<span class = "m-2 p-2">Counter: <t t-esc="state.value"/></span>
			<button class="btn-primary" t-on-click="increment">Increase by 1</button>
			<button class="btn-primary" t-on-click="reset_to_zero">Reset to 1</button>
		</div>
    </t>

</templates>
